{
    "name": "temirkhan/flush-listener",
    "description": "Entity manager flush listener",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Temirkhan\\FlushListenerBundle\\": "src/FlushListenerBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Temirkhan\\FlushListenerBundle\\": "test/FlushListenerBundle/"
        }
    },
    "require": {
        "php": ">=7.0",
        "doctrine/orm": "^2.5",
        "symfony/http-kernel": "3.3-4.2",
        "symfony/config": "3.3-4.2",
        "symfony/dependency-injection": "3.3-4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    }
}
